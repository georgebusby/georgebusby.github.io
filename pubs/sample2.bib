@article{busbyAncestryspecificPolygenicRisk2023,
  title = {Ancestry-Specific Polygenic Risk Scores Are Risk Enhancers for Clinical Cardiovascular Disease Assessments},
  author = {\textbf{Busby}, \textbf{George B.} and Kulm, Scott and Bolli, Alessandro and Kintzle, Jen and Domenico, Paolo Di and Bott{\`a}, Giordano},
