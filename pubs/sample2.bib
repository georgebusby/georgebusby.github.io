@article{bandInsightsMalariaSusceptibility2019,
	title = {Insights into malaria susceptibility using genome-wide data on 17,000 individuals from {Africa}, {Asia} and {Oceania}},
	volume = {10},
	issn = {2041-1723},
	url = {https://doi.org/10.1038/s41467-019-13480-z},
	doi = {10.1038/s41467-019-13480-z},
	abstract = {The human genetic factors that affect resistance to infectious disease are poorly understood. Here we report a genome-wide association study in 17,000 severe malaria cases and population controls from 11 countries, informed by sequencing of family trios and by direct typing of candidate loci in an additional 15,000 samples. We identify five replicable associations with genome-wide levels of evidence including a newly implicated variant on chromosome 6. Jointly, these variants account for around one-tenth of the heritability of severe malaria, which we estimate as {\textasciitilde}23\% using genome-wide genotypes. We interrogate available functional data and discover an erythroid-specific transcription start site underlying the known association in ATP2B4, but are unable to identify a likely causal mechanism at the chromosome 6 locus.  Previously reported HLA associations do not replicate in these samples. This large dataset will provide a foundation for further research on the genetic determinants of malaria resistance in diverse populations.},
	number = {1},
	journal = {\color{emphasis}{\textbf{Nature Communications}}},
	author = {Band, Gavin and Le, Quang Si and Clarke, Geraldine M. and Kivinen, Katja and Hubbart, Christina and Jeffreys, Anna E. and Rowlands, Kate and Leffler, Ellen M. and Jallow, Muminatou and Conway, David J. and Sisay-Joof, Fatoumatta and Sirugo, Giorgio and d’Alessandro, Umberto and Toure, Ousmane B. and Thera, Mahamadou A. and Konate, Salimata and Sissoko, Sibiri and Mangano, Valentina D. and Bougouma, Edith C. and Sirima, Sodiomon B. and Amenga-Etego, Lucas N. and Ghansah, Anita K. and Hodgson, Abraham V. O. and Wilson, Michael D. and Enimil, Anthony and Ansong, Daniel and Evans, Jennifer and Ademola, Subulade A. and Apinjoh, Tobias O. and Ndila, Carolyne M. and Manjurano, Alphaxard and Drakeley, Chris and Reyburn, Hugh and Phu, Nguyen Hoan and Quyen, Nguyen Thi Ngoc and Thai, Cao Quang and Hien, Tran Tinh and Teo, Yik Ying and Manning, Laurens and Laman, Moses and Michon, Pascal and Karunajeewa, Harin and Siba, Peter and Allen, Steve and Allen, Angela and Bahlo, Melanie and Davis, Timothy M. E. and Simpson, Victoria and Shelton, Jennifer and Spencer, Chris C. A. and Busby, George B. J. and Kerasidou, Angeliki and Drury, Eleanor and Stalker, Jim and Dilthey, Alexander and Mentzer, Alexander J. and McVean, Gil and Bojang, Kalifa A. and Doumbo, Ogobara and Modiano, David and Koram, Kwadwo A. and Agbenyega, Tsiri and Amodu, Olukemi K. and Achidi, Eric and Williams, Thomas N. and Marsh, Kevin and Riley, Eleanor M. and Molyneux, Malcolm and Taylor, Terrie and Dunstan, Sarah J. and Farrar, Jeremy and Mueller, Ivo and Rockett, Kirk A. and Kwiatkowski, Dominic P. and {Malaria Genomic Epidemiology Network}},
