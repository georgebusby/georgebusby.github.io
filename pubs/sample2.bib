	pages = {884 --892},
	file = {Full Text PDF:/home/george/.mozilla/firefox/3zejrt86.default/zotero/storage/ZNWGTHX7/Busby et al. - 2012 - The peopling of Europe and the cautionary tale of .pdf:application/pdf;Snapshot:/home/george/.mozilla/firefox/3zejrt86.default/zotero/storage/RSWTSI9M/884.html:text/html}
}

