	keywords = {Biological sciences, GENETICS}
}

@article{tofanelli_greeks_2015,
	title = {The {Greeks} in the {West}: genetic signatures of the {Hellenic} colonisation in southern {Italy} and {Sicily}},
	issn = {1476-5438},
	shorttitle = {The {Greeks} in the {West}},
	doi = {10.1038/ejhg.2015.124},
	abstract = {Greek colonisation of South Italy and Sicily (Magna Graecia) was a defining event in European cultural history, although the demographic processes and genetic impacts involved have not been systematically investigated. Here, we combine high-resolution surveys of the variability at the uni-parentally inherited Y chromosome and mitochondrial DNA in selected samples of putative source and recipient populations with forward-in-time simulations of alternative demographic models to detect signatures of that impact. Using a subset of haplotypes chosen to represent historical sources, we recover a clear signature of Greek ancestry in East Sicily compatible with the settlement from Euboea during the Archaic Period (eighth to fifth century BCE). We inferred moderate sex-bias in the numbers of individuals involved in the colonisation: a few thousand breeding men and a few hundred breeding women were the estimated number of migrants. Last, we demonstrate that studies aimed at quantifying Hellenic genetic flow by the proportion of specific lineages surviving in present-day populations may be misleading.European Journal of Human Genetics advance online publication, 15 July 2015; doi:10.1038/ejhg.2015.124.},
	language = {ENG},
	journal = {\color{emphasis}{\textbf{European Journal of Human Genetics}}},
	author = {Tofanelli, Sergio and Brisighelli, Francesca and Anagnostou, Paolo and \textbf{Busby}, \textbf{George} and Ferri, Gianmarco and Thomas, Mark G. and Taglioli, Luca and Rudan, Igor and Zemunik, Tatijana and Hayward, Caroline and Bolnick, Deborah and Romano, Valentino and Cali, Francesco and Luiselli, Donata and Shepherd, Gillian B. and Tusa, Sebastiano and Facella, Antonino and Capelli, Cristian},
	month = jul,
	year = {2015},
	pmid = {26173964}
}

@article{martinez-cadenas_relationship_2015,
	title = {The relationship between surname frequency and {Y} chromosome variation in {Spain}},
	copyright = {© 2015 Nature Publishing Group},
	issn = {1018-4813},
	url = {http://www.nature.com/ejhg/journal/vaop/ncurrent/full/ejhg201575a.html},
	doi = {10.1038/ejhg.2015.75},
	abstract = {In most societies, surnames are passed down from fathers to sons, just like the Y chromosome. It follows that, theoretically, men sharing the same surnames would also be expected to share related Y chromosomes. Previous investigations have explored such relationships, but so far, the only detailed studies that have been conducted are on samples from the British Isles. In order to provide additional insights into the correlation between surnames and Y chromosomes, we focused on the Spanish population by analysing Y chromosomes from 2121 male volunteers representing 37 surnames. The results suggest that the degree of coancestry within Spanish surnames is highly dependent on surname frequency, in overall agreement with British but not Irish surname studies. Furthermore, a reanalysis of comparative data for all three populations showed that Irish surnames have much greater and older surname descent clusters than Spanish and British ones, suggesting that Irish surnames may have considerably earlier origins than Spanish or British ones. Overall, despite closer geographical ties between Ireland and Britain, our analysis points to substantial similarities in surname origin and development between Britain and Spain, while possibly hinting at unique demographic or social events shaping Irish surname foundation and development.},
	language = {en},
	urldate = {2015-04-23},
	journal = {\color{emphasis}{\textbf{European Journal of Human Genetics}}},
	author = {Martinez-Cadenas, Conrado and Blanco-Verea, Alejandro and Hernando, Barbara and \textbf{Busby}, \textbf{George} and Brion, Maria and Carracedo, Angel and Salas, Antonio and Capelli, Cristian},
	month = apr,
	year = {2015}
}


@article{busby_role_2015,
	title = {The {Role} of {Recent} {Admixture} in {Forming} the {Contemporary} {West} {Eurasian} {Genomic} {Landscape}},
	volume = {25},
	issn = {0960-9822},
	url = {http://www.cell.com/article/S0960982215009495/abstract},
	doi = {10.1016/j.cub.2015.08.007},
	abstract = {Over the past few years, studies of DNA isolated from human fossils and archaeological remains have generated considerable novel insight into the historyof our species. Several landmark papers have described the genomes of ancient humans across West Eurasia, demonstrating the presence of large-scale, dynamic population movements over the last 10,000 years, such that ancestry across present-day populations is likely to be a mixture of several ancient groups [ 1–7 ]. While these efforts are bringing the details of West Eurasian prehistory into increasing focus, studies aimed at understanding the processes behind the generation of the current West Eurasian genetic landscape have been limited by the number of populations sampled or have been either too regional or global in their outlook [ 8–11 ]. Here, using recently described haplotype-based techniques [ 11 ], we present the results of a systematic survey of recent admixture history across Western Eurasia and show that admixture is a universal property across almost all groups. Admixture in all regions except North Western Europe involved the influx of genetic material from outside of West Eurasia, which we date to specific time periods. Within Northern, Western, and Central Europe, admixture tended to occur between local groups during the period 300 to 1200 CE. Comparisons of the genetic profiles of West Eurasians before and after admixture show that population movements within the last 1,500 years are likely to have maintained differentiation among groups. Our analysis provides a timeline of the gene flow events that have generated the contemporary genetic landscape of West Eurasia.},
	language = {English},
	number = {19},
	urldate = {2015-10-14},
	journal = {\color{emphasis}{\textbf{Current Biology}}},
	author = {\textbf{Busby}, \textbf{George} and Hellenthal, Garrett and Montinaro, Francesco and Tofanelli, Sergio and Bulayeva, Kazima and Rudan, Igor and Zemunik, Tatijana and Hayward, Caroline and Toncheva, Draga and Karachanak-Yankova, Sena and Nesheva, Desislava and Anagnostou, Paolo and Cali, Francesco and Brisighelli, Francesca and Romano, Valentino and Lefranc, Gerard and Buresi, Catherine and Ben Chibani, Jemni and Haj-Khelil, Amel and Denden, Sabri and Ploski, Rafal and Krajewski, Pawel and Hervig, Tor and Moen, Torolf and Herrera, Rene J. and Wilson, James F. and Myers, Simon and Capelli, Cristian},
	month = may,
	year = {2015},
	pages = {2518--2526},
	file = {Full Text PDF:/home/george/.mozilla/firefox/3zejrt86.default/zotero/storage/54VH8USA/Busby et al. - 2015 - The Role of Recent Admixture in Forming the Contem.pdf:application/pdf;Snapshot:/home/george/.mozilla/firefox/3zejrt86.default/zotero/storage/G7TPB333/S0960-9822(15)00949-5.html:text/html}
}

@article{gonzalez-santos_genome-wide_2015,
	title = {Genome-{Wide} {SNP} {Analysis} of {Southern} {African} {Populations} {Provides} {New} {Insights} into the {Dispersal} of {Bantu}-{Speaking} {Groups}},
	volume = {7},
	issn = {, 1759-6653},
	url = {http://gbe.oxfordjournals.org/content/7/9/2560},
	doi = {10.1093/gbe/evv164},
	abstract = {The expansion of Bantu-speaking agropastoralist populations had a great impact on the genetic, linguistic, and cultural variation of sub-Saharan Africa. It is generally accepted that Bantu languages originated in an area around the present border between Cameroon and Nigeria approximately 5,000 years ago, from where they spread South and East becoming the largest African linguistic branch. The demic consequences of this event are reflected in the relatively high genetic homogeneity observed across most of sub-Saharan Africa populations. In this work, we explored genome-wide single nucleotide polymorphism data from 28 populations to characterize the genetic components present in sub-Saharan African populations. Combining novel data from four Southern African populations with previously published results, we reject the hypothesis that the “non-Bantu” genetic component reported in South-Eastern Africa (Mozambique) reflects extensive gene flow between incoming agriculturalist and resident hunter-gatherer communities. We alternatively suggest that this novel component is the result of demographic dynamics associated with the Bantu dispersal.},
	language = {en},
	number = {9},
	urldate = {2015-09-20},
	journal = {\color{emphasis}{\textbf{Genome Biology and Evolution}}},
	author = {González-Santos, Miguel and Montinaro, Francesco and Oosthuizen, Ockie and Oosthuizen, Erica and \textbf{Busby}, \textbf{George} and Anagnostou, Paolo and Destro-Bisol, Giovanni and Pascali, Vincenzo and Capelli, Cristian},
	month = sep,
	year = {2015},
	pmid = {26363465},
	keywords = {admixture, Bantu speakers, genome-wide SNPs, Southern Africa},
	pages = {2560--2568}
}

